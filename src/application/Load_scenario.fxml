<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.scene.control.*?>
<?import java.lang.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8" fx:controller="application.LoadPopUpController">
   <center>
      <Pane prefHeight="77.0" prefWidth="396.0" BorderPane.alignment="CENTER">
         <children>
            <AnchorPane layoutX="-1.0" prefHeight="77.0" prefWidth="377.0">
               <children>
                  <Label layoutX="14.0" layoutY="30.0" text="New Scenario to Load:">
                     <font>
                        <Font name="System Bold" size="12.0" />
                     </font>
                  </Label>
                  <ComboBox fx:id="scenario_combobox" layoutX="161.0" layoutY="26.0" onAction="#initter" onMousePressed="#initter" prefHeight="25.0" prefWidth="148.0" promptText="Choose a Scenario" />
                  <Button fx:id="scenario_load" layoutX="329.0" layoutY="26.0" mnemonicParsing="false" onAction="#scenario_load_action" text="Load" />
               </children>
            </AnchorPane>
         </children>
      </Pane>
   </center>
</BorderPane>
